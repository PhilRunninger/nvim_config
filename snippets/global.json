{
    "date - mm/dd/yy": {
        "prefix": "dateUS",
        "description": "Date in US format",
        "body": ["${CURRENT_MONTH}/${CURRENT_DATE}/${CURRENT_YEAR}"]
    },
    "time - hh:mm:ss": {
        "prefix": "timeSec",
        "description": "Time with seconds",
        "body": ["${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"]
    },
    "date - for journal entry": {
        "prefix": "dateJournal",
        "description": "Day of month for my journal",
        "body": ["* **${CURRENT_DATE}**: "]
    }
}

